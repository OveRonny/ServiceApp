{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/service-worker.js",
      "/manifest.json"
    ]
  },

  "routes": [
    {
      "route": "/index.html",
      "headers": {
        "cache-control": "no-cache, must-revalidate"
      }
    },
    {
      "route": "/service-worker.js",
      "headers": {
        "cache-control": "no-cache, must-revalidate"
      }
    },
    {
      "route": "/manifest.json",
      "headers": {
        "cache-control": "no-cache, must-revalidate"
      }
    },
    {
      "route": "/_framework/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.dll",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.wasm",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.js",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.css",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ],

  "mimeTypes": {
    ".wasm": "application/wasm"
  }
}

