{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [ "/service-worker.js", "/manifest.json" ]
  },

  "responseOverrides": {
    "/index.html": {
      "headers": {
        "Cache-Control": "no-cache, must-revalidate"
      }
    },
    "/service-worker.js": {
      "headers": {
        "Cache-Control": "no-cache, must-revalidate"
      }
    },
    "/manifest.json": {
      "headers": {
        "Cache-Control": "no-cache, must-revalidate"
      }
    },
    "/_framework/*": {
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    "/*.dll": {
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    "/*.wasm": {
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    "/*.js": {
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    "/*.css": {
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  },

  "mimeTypes": {
    ".wasm": "application/wasm"
  }
}
